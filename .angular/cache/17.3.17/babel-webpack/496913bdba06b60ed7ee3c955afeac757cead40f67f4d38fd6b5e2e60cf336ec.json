{"ast":null,"code":"import { __decorate } from \"tslib\";\nimport { Component } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule } from '@angular/forms';\nlet ContactComponent = class ContactComponent {\n  constructor() {\n    this.title = 'Contact - Legacy Donation';\n    this.contactFormData = {\n      name: '',\n      email: '',\n      phone: '',\n      subject: '',\n      message: ''\n    };\n  }\n  onSubmit() {\n    console.log('Contact form submitted:', this.contactFormData);\n    alert('Thank you for your message! We will get back to you soon.');\n    // Reset form\n    this.contactFormData = {\n      name: '',\n      email: '',\n      phone: '',\n      subject: '',\n      message: ''\n    };\n  }\n};\nContactComponent = __decorate([Component({\n  selector: 'app-contact',\n  standalone: true,\n  imports: [CommonModule, FormsModule],\n  template: `\n    <div class=\"py-20\">\n      <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div class=\"text-center\">\n          <h2 class=\"text-3xl font-extrabold text-gray-900 sm:text-4xl\">\n            Contact Us\n          </h2>\n          <p class=\"mt-4 text-lg text-gray-500\">\n            Get in touch with us. We're here to help and answer any question you might have.\n          </p>\n        </div>\n        \n        <div class=\"mt-16 grid grid-cols-1 gap-8 lg:grid-cols-2\">\n          <!-- Contact Information -->\n          <div>\n            <h3 class=\"text-2xl font-bold text-gray-900 mb-6\">Get in Touch</h3>\n            <div class=\"space-y-6\">\n              <div class=\"flex\">\n                <div class=\"flex-shrink-0\">\n                  <svg class=\"h-6 w-6 text-primary-600\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z\" />\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 11a3 3 0 11-6 0 3 3 0 016 0z\" />\n                  </svg>\n                </div>\n                <div class=\"ml-3\">\n                  <h4 class=\"text-lg font-medium text-gray-900\">Office Address</h4>\n                  <p class=\"mt-1 text-base text-gray-500\">\n                    123 Charity Lane<br>\n                    Suite 100<br>\n                    Help City, HC 12345\n                  </p>\n                </div>\n              </div>\n              \n              <div class=\"flex\">\n                <div class=\"flex-shrink-0\">\n                  <svg class=\"h-6 w-6 text-primary-600\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z\" />\n                  </svg>\n                </div>\n                <div class=\"ml-3\">\n                  <h4 class=\"text-lg font-medium text-gray-900\">Phone</h4>\n                  <p class=\"mt-1 text-base text-gray-500\">\n                    <a href=\"tel:+15551234567\" class=\"hover:text-primary-600\">(555) 123-4567</a>\n                  </p>\n                </div>\n              </div>\n              \n              <div class=\"flex\">\n                <div class=\"flex-shrink-0\">\n                  <svg class=\"h-6 w-6 text-primary-600\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M3 8l7.89 7.89a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z\" />\n                  </svg>\n                </div>\n                <div class=\"ml-3\">\n                  <h4 class=\"text-lg font-medium text-gray-900\">Email</h4>\n                  <p class=\"mt-1 text-base text-gray-500\">\n                    <a href=\"mailto:info@legacydonation.org\" class=\"hover:text-primary-600\">info@legacydonation.org</a>\n                  </p>\n                </div>\n              </div>\n              \n              <div class=\"flex\">\n                <div class=\"flex-shrink-0\">\n                  <svg class=\"h-6 w-6 text-primary-600\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z\" />\n                  </svg>\n                </div>\n                <div class=\"ml-3\">\n                  <h4 class=\"text-lg font-medium text-gray-900\">Business Hours</h4>\n                  <p class=\"mt-1 text-base text-gray-500\">\n                    Monday - Friday: 9:00 AM - 6:00 PM<br>\n                    Saturday: 10:00 AM - 4:00 PM<br>\n                    Sunday: Closed\n                  </p>\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- Contact Form -->\n          <div>\n            <h3 class=\"text-2xl font-bold text-gray-900 mb-6\">Send us a Message</h3>\n            <form (ngSubmit)=\"onSubmit()\" #contactForm=\"ngForm\" class=\"space-y-6\">\n              <div>\n                <label class=\"block text-sm font-medium text-gray-700\">Full Name *</label>\n                <input type=\"text\" \n                       name=\"name\"\n                       required\n                       [(ngModel)]=\"contactFormData.name\"\n                       class=\"mt-1 focus:ring-primary-500 focus:border-primary-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\">\n              </div>\n              \n              <div>\n                <label class=\"block text-sm font-medium text-gray-700\">Email Address *</label>\n                <input type=\"email\" \n                       name=\"email\"\n                       required\n                       [(ngModel)]=\"contactFormData.email\"\n                       class=\"mt-1 focus:ring-primary-500 focus:border-primary-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\">\n              </div>\n              \n              <div>\n                <label class=\"block text-sm font-medium text-gray-700\">Phone Number</label>\n                <input type=\"tel\" \n                       name=\"phone\"\n                       [(ngModel)]=\"contactFormData.phone\"\n                       class=\"mt-1 focus:ring-primary-500 focus:border-primary-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\">\n              </div>\n              \n              <div>\n                <label class=\"block text-sm font-medium text-gray-700\">Subject *</label>\n                <select name=\"subject\" \n                        required\n                        [(ngModel)]=\"contactFormData.subject\"\n                        class=\"mt-1 block w-full py-2 px-3 border border-gray-300 bg-white rounded-md shadow-sm focus:outline-none focus:ring-primary-500 focus:border-primary-500 sm:text-sm\">\n                  <option value=\"\">Select a subject</option>\n                  <option value=\"general\">General Inquiry</option>\n                  <option value=\"donation\">Donation Question</option>\n                  <option value=\"assistance\">Request Assistance</option>\n                  <option value=\"partnership\">Partnership Opportunity</option>\n                  <option value=\"volunteer\">Volunteer Information</option>\n                </select>\n              </div>\n              \n              <div>\n                <label class=\"block text-sm font-medium text-gray-700\">Message *</label>\n                <textarea name=\"message\" \n                          rows=\"4\" \n                          required\n                          [(ngModel)]=\"contactFormData.message\"\n                          class=\"mt-1 focus:ring-primary-500 focus:border-primary-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\"\n                          placeholder=\"Tell us how we can help you...\"></textarea>\n              </div>\n              \n              <div>\n                <button type=\"submit\" \n                        [disabled]=\"!contactForm.valid\"\n                        class=\"w-full flex justify-center py-2 px-4 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-primary-600 hover:bg-primary-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-primary-500 disabled:opacity-50\">\n                  Send Message\n                </button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  `,\n  styles: []\n})], ContactComponent);\nexport { ContactComponent };","map":null,"metadata":{},"sourceType":"module","externalDependencies":[]}